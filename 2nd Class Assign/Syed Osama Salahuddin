       <!-- task 9 -->
       
       <!-- <script>
        
           var EnglishMarks = prompt("Please Enter English Marks");
           EnglishMarks = parseFloat(EnglishMarks);

           var MathsMarks = prompt("Please Enter English Marks");
           MathsMarks = parseFloat(MathsMarks);

           var UrduMarks = prompt("Please Enter English Marks");
           UrduMarks = parseFloat(UrduMarks);
            var fullmarks = 300;
           var totalMarks = EnglishMarks + MathsMarks + UrduMarks;

           var Average = totalMarks/300;

           var percentage = Average * 100;

           if (percentage >= 80)
           {
               document.write("<b>Total Marks</b>:"+fullmarks+"<br>"+"<b>Marks Obtained</b>:"+totalMarks+"<br>"+"<b>Grade</b>: A-One"+"<br>"+"<b>Percentage</b>:"+percentage+"%"+"<br>"+"<b>Remarks</b>: Excellent");
           }
           else if (percentage >= 70)
           {
               document.write("<b>Total Marks</b>:"+fullmarks+"<br>"+"<b>Marks Obtained</b>:"+totalMarks+"<br>"+"<b>Grade</b>: A"+"<br>"+"<b>Percentage</b>:"+percentage+"%"+"<br>"+"<b>Remarks</b>: Good");
           }
           else if (percentage >= 60)
           {
               document.write("<b>Total Marks</b>:"+fullmarks+"<br>"+"<b>Marks Obtained</b>:"+totalMarks+"<br>"+"<b>Grade</b>: B"+"<br>"+"<b>Percentage</b>:"+percentage+"%"+"<br>"+"<b>Remarks</b>: You need to improve");
           }
           else if(percentage>100){
               document.write("Invalid Numbers")

           }
           else{
               document.write("Sorry");
           }


        </script> -->

        <!-- task 10
        <script>
         
         

         var priceofitemone = 700;
         var priceofitemtwo = 1000;

         var quantityofitemOne = prompt("Please Enter Quantity Of USB");
         quantityofitemOne=parseInt(quantityofitemOne);

         var sumofitemOne = priceofitemone * quantityofitemOne; 

         var quantityofitemtwo = prompt("Please Enter Quantity Of T-Shirt");
         quantityofitemtwo=parseInt(quantityofitemtwo);

         var sumofitemTwo = priceofitemtwo * quantityofitemtwo;

         var shippingcharges = 250;

         var totalOrder = sumofitemOne + sumofitemTwo + shippingcharges;

         var Discounted = totalOrder* 0.1;

         Discounted= totalOrder - Discounted;

         document.write("Price Of T-Shirt is "+priceofitemtwo+"<br>");
         document.write("Price Of USB is "+priceofitemone+"<br>");
         document.write("Quantiy Of T-Shirt is "+quantityofitemtwo+"<br>");
         document.write("Quantiy Of USB is "+quantityofitemOne+"<br>");
         document.write("Shipping Charges is "+shippingcharges+"<br>");
         document.write("Quantiy Of T-Shirt is "+quantityofitemtwo+"<br>");
         document.write("Total Cost Of Your Order is "+totalOrder+" PKR<br>");
         document.write("Discounted Price is "+Discounted+" PKR<br>");

         
        
        </script> -->

