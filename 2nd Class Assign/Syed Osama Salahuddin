        var teamOneName = prompt("Enter Team One Name");
        var teamTwoName = prompt("Enter Team Two Name");

        var teamOneScore = prompt("Enter Team One Score");
        teamOneScore=parseInt(teamOneScore);

        var teamTwoScore = prompt("Enter Team Two Score");
        teamTwoScore=parseInt(teamTwoScore);

        if( teamOneScore > teamTwoScore)
        {
            alert(teamOneName+" has Won the match")
        }
        else if( teamOneScore < teamTwoScore)
        {
            alert(teamTwoName+" has Won the match")
        }
        else if (teamOneScore = teamTwoScore)
        {
            alert("Tied")
